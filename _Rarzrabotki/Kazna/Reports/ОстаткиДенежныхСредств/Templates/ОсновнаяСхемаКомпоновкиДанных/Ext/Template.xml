		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ДенежныеСредстваОстатки.ВидДенежныхСредств КАК ВидДенежныхСредств,
	ДенежныеСредстваОстатки.БанковскийСчетКасса КАК БанковскийСчетКасса,
	ДенежныеСредстваОстатки.Организация КАК Организация,
	ДенежныеСредстваОстатки.БанковскийСчетКасса._Направление КАК Направление,
	ВЫБОР
		КОГДА ДенежныеСредстваОстатки.БанковскийСчетКасса.Валюта = ЗНАЧЕНИЕ(Справочник.Валюты.ПустаяСсылка)
			ТОГДА ДенежныеСредстваОстатки.СуммаОстаток
		ИНАЧЕ ДенежныеСредстваОстатки.СуммаОстаток * ЕСТЬNULL(КурсыВалютСрезПоследних.Курс, 1)
	КОНЕЦ КАК Остаток,
	ВЫБОР
		КОГДА ДенежныеСредстваОстатки.БанковскийСчетКасса.Валюта = ЗНАЧЕНИЕ(Справочник.Валюты.ПустаяСсылка)
			ТОГДА &amp;Гривна
		ИНАЧЕ ДенежныеСредстваОстатки.БанковскийСчетКасса.Валюта
	КОНЕЦ КАК Валюта,
	ДенежныеСредстваОстатки.БанковскийСчетКасса.Банк КАК Банк,
	ДенежныеСредстваОстатки.СуммаОстаток КАК ОстатокВВалюте
ИЗ
	РегистрНакопления.ДенежныеСредства.Остатки(&amp;Период, ) КАК ДенежныеСредстваОстатки
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.КурсыВалют.СрезПоследних(&amp;Период, ) КАК КурсыВалютСрезПоследних
		ПО ДенежныеСредстваОстатки.БанковскийСчетКасса.Валюта = КурсыВалютСрезПоследних.Валюта
ГДЕ
	ЕСТЬNULL(ДенежныеСредстваОстатки.БанковскийСчетКасса.Наименование, 0) &lt;&gt; 0</query>
