		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	БДДСОбороты.Подразделение,
	БДДСОбороты.СтатьяДвиженияДенежныхСредств,
	БДДСОбороты.ВидОплаты,
	ВЫБОР
		КОГДА БДДСОбороты.СтатьяДвиженияДенежныхСредств В ИЕРАРХИИ (&amp;ГруппаДоход)
				И БДДСОбороты.НаправлениеДДС = &amp;НаправлениеДДСПриход
			ТОГДА БДДСОбороты.СуммаПриходОборот + БДДСОбороты.СуммаРасходОборот
		КОГДА БДДСОбороты.СтатьяДвиженияДенежныхСредств В ИЕРАРХИИ (&amp;ГруппаДоход)
				И БДДСОбороты.НаправлениеДДС = &amp;НаправлениеДДСРасход
			ТОГДА (БДДСОбороты.СуммаПриходОборот + БДДСОбороты.СуммаРасходОборот)
		КОГДА БДДСОбороты.СтатьяДвиженияДенежныхСредств В ИЕРАРХИИ (&amp;ГруппаРасход)
				И БДДСОбороты.НаправлениеДДС = &amp;НаправлениеДДСРасход
			ТОГДА (БДДСОбороты.СуммаПриходОборот + БДДСОбороты.СуммаРасходОборот)
		КОГДА БДДСОбороты.СтатьяДвиженияДенежныхСредств В ИЕРАРХИИ (&amp;ГруппаРасход)
				И БДДСОбороты.НаправлениеДДС = &amp;НаправлениеДДСПриход
			ТОГДА БДДСОбороты.СуммаПриходОборот + БДДСОбороты.СуммаРасходОборот
		ИНАЧЕ 0
	КОНЕЦ КАК СуммаИтого,
	0 КАК СуммаПлан,
	БДДСОбороты.Регистратор,
	БДДСОбороты.Регистратор.Контрагент КАК Контрагент,
	БДДСОбороты.Направление
ИЗ
	РегистрНакопления.БДДС.Обороты(
			,
			,
			Авто,
			ВЫБОР
				КОГДА &amp;ОтображатьВсеСтатьи
						И &amp;НеВыводитьФинДеятельность
					ТОГДА СтатьяДвиженияДенежныхСредств.ЭтоФинансоваяДеятельность = ЛОЖЬ
				ИНАЧЕ СтатьяДвиженияДенежныхСредств.ЭтоФинансоваяДеятельность = ЛОЖЬ
						ИЛИ &amp;ОтображатьВсеСтатьи
			КОНЕЦ {(Подразделение В ИЕРАРХИИ (&amp;Подразделение))}) КАК БДДСОбороты

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	БДДСОбороты.Подразделение,
	БДДСОбороты.ПоказательБюджета,
	БДДСОбороты.ВидОплаты,
	0,
	ВЫБОР
		КОГДА БДДСОбороты.ПоказательБюджета В ИЕРАРХИИ (&amp;ГруппаДоход)
			ТОГДА БДДСОбороты.СуммаОборот
		КОГДА БДДСОбороты.ПоказательБюджета В ИЕРАРХИИ (&amp;ГруппаРасход)
			ТОГДА БДДСОбороты.СуммаОборот
		ИНАЧЕ 0
	КОНЕЦ,
	БДДСОбороты.Регистратор,
	NULL,
	БДДСОбороты.Направление
ИЗ
	РегистрНакопления.БюджетыНаМесяц.Обороты(
			,
			,
			Авто,
			ВЫБОР
					КОГДА &amp;ОтображатьВсеСтатьи
							И &amp;НеВыводитьФинДеятельность
						ТОГДА ПоказательБюджета.ЭтоФинансоваяДеятельность = ЛОЖЬ
					ИНАЧЕ &amp;ОтображатьВсеСтатьи
							ИЛИ ПоказательБюджета.ЭтоФинансоваяДеятельность = ЛОЖЬ
				КОНЕЦ
				И ВидПериода = ЗНАЧЕНИЕ(Перечисление.ВидыПериодовБюджетирования.МЕСЯЦ) {(Подразделение В ИЕРАРХИИ (&amp;Подразделение))}) КАК БДДСОбороты</query>
