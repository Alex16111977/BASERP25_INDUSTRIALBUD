		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Т_ДокСчета.Ссылка КАК Счет_Ссылка,
	РН_ДС.Регистратор КАК ППИСсылка,
	РН_ДС.Сумма КАК СуммаОплаты,
	РН_ДС.Период КАК ДатаОплаты,
	Т_ДокСчета.СуммаДокумента КАК СуммаСчета,
	Т_ДокСчета.Контрагент КАК Контрагент,
	РН_Счета.СуммаОстаток КАК ТребуетсяЗаявка,
	РН_Счета.НаОплатуОстаток КАК ТребуетсяОплата,
	РН_Счета.ВКБОстаток КАК ВыгруженаВКБ,
	Т_ДокСчета.Дата КАК Счет_Дата,
	Т_ДокСчета.Номер КАК Счет_Номер,
	ВЫБОР
		КОГДА НЕ Т_ДокСчета.Виза_Состояние = ЗНАЧЕНИЕ(Перечисление.Визы_Состояния.Утвержден)
			ТОГДА Т_ДокСчета.Виза_Состояние
		КОГДА ЕСТЬNULL(РН_ДС.Сумма, 0) &gt;= Т_ДокСчета.СуммаДокумента
			ТОГДА ЗНАЧЕНИЕ(Перечисление.Визы_Состояния.Оплачено)
		КОГДА ЕСТЬNULL(РН_ДС.Сумма, 0) &gt; 0
			ТОГДА ЗНАЧЕНИЕ(Перечисление.Визы_Состояния.ОплаченоЧастично)
		КОГДА ЕСТЬNULL(РН_Счета.ВКБОстаток, 0) &gt; 0
			ТОГДА ЗНАЧЕНИЕ(Перечисление.Визы_Состояния.ВыгруженаВКБ)
		КОГДА ЕСТЬNULL(РН_Счета.НаОплатуОстаток, 0) &gt; 0
			ТОГДА ЗНАЧЕНИЕ(Перечисление.Визы_Состояния.ТребуетсяОплата)
		КОГДА ЕСТЬNULL(РН_Счета.СуммаОстаток, 0) &gt; 0
			ТОГДА ЗНАЧЕНИЕ(Перечисление.Визы_Состояния.ТребуетсяЗаявка)
		ИНАЧЕ Т_ДокСчета.Виза_Состояние
	КОНЕЦ КАК Счет_СводноеСостояние,
	Т_ДокСчета.Виза_Состояние КАК Счет_Состояние,
	РН_ДС.Организация,
	РН_ДС.ВидДенежныхСредств,
	РН_ДС.БанковскийСчетКасса
ИЗ
	Документ.СчетНаОплатуПоставщика КАК Т_ДокСчета
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.СчетаПоставщиков.Остатки КАК РН_Счета
		ПО (РН_Счета.СчетНаОплатуПоставщика = Т_ДокСчета.Ссылка)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ДенежныеСредства КАК РН_ДС
		ПО (РН_ДС.Регистратор.ДокументОснование.ДокументОснование = Т_ДокСчета.Ссылка)
ГДЕ
	РН_ДС.Регистратор.Дата МЕЖДУ &amp;ДатаНач И &amp;Период</query>
