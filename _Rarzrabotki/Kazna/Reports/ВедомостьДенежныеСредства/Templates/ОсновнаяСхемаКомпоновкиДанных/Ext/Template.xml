	Т.СуммаНачальныйОстаток,
	Т.СуммаПриход,
	Т.СуммаРасход,
	Т.СуммаКонечныйОстаток,
	Т.ПериодДень,
	ВЫБОР
		КОГДА Т.Регистратор = НЕОПРЕДЕЛЕНО
			ТОГДА NULL
		ИНАЧЕ Т.Регистратор
	КОНЕЦ КАК Регистратор,
	Т.ПериодМесяц,
	Т.ПериодНеделя,
	Т.ПериодДень КАК ПериодДень1,
	Т.ПериодСекунда
ИЗ
	РегистрНакопления.ДенежныеСредства.ОстаткиИОбороты(, , Авто, , ) КАК Т</query>
