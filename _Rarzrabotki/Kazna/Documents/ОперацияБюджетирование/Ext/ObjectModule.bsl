	// регистр БДДС 
	Движения.БДДС.Записывать = Истина;
	Для Каждого ТекСтрокаСоставБДДС Из СоставБДДС Цикл
		Движение = Движения.БДДС.Добавить();
		Движение.Период = Дата;
		Движение.Подразделение = ТекСтрокаСоставБДДС.Подразделение;
		Движение.СтатьяДвиженияДенежныхСредств = ТекСтрокаСоставБДДС.СтатьяДвиженияДенежныхСредств;
		Движение.ПланФакт = ТекСтрокаСоставБДДС.ПланФакт;
		Движение.ВидОплаты = ТекСтрокаСоставБДДС.ВидОплаты;
		Движение.ДокументОснование = ТекСтрокаСоставБДДС.ДокументОснование;
		Движение.НаправлениеДДС = ТекСтрокаСоставБДДС.НаправлениеДДС;
		Движение.Направление = ТекСтрокаСоставБДДС.Направление;
		Движение.Сумма = ТекСтрокаСоставБДДС.Сумма;
		Движение.Комментарий = Комментарий;   
		Если Движение.НаправлениеДДС =Перечисления.НаправленияДвиженийДенежныхСредств.Приход Тогда
			Движение.СуммаПриход =Движение.Сумма ;
		Иначе	
			Движение.СуммаРасход=Движение.Сумма ;
		КонецЕсли;
	КонецЦикла;

