				ТабЗн = ДанныеПечати.Контрагент.КонтактнаяИнформация.Выгрузить();
				
				НайденныеСтроки = ТабЗн.НайтиСтроки(Новый Структура("Тип,Вид", Перечисления.ТипыКонтактнойИнформации.Адрес, ВидАдреса));
				
