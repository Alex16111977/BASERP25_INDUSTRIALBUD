// Формирует соответствие имеи регистра и элементов формы, связанных с регистром.
// 
// Параметры:
// Возвращаемое значение:
// 	Соответствие - Соответствие элементоф формы
Функция СоответствиеЭлементовФормыПараметрамНалоговУчетныхПолитик() Экспорт
	
	СоответствиеЭлементов = Новый Соответствие();
	
	МассивЭлементов = Новый Массив();
	МассивЭлементов.Добавить("ГруппаСкопироватьУчетнаяПолитикаФинансовогоУчета");
	МассивЭлементов.Добавить("СкопироватьУчетнаяПолитикаФинансовогоУчета");
	МассивЭлементов.Добавить("ГруппаУчетнаяПолитикаФинансовогоУчетаОтступ");
	МассивЭлементов.Добавить("ОтступУчетнаяПолитикаФинансовогоУчета");
	МассивЭлементов.Добавить("ОписаниеУчетнаяПолитикаФинансовогоУчета");
	
	СоответствиеЭлементов.Вставить("УчетнаяПолитикаФинансовогоУчета", МассивЭлементов);
	
	МассивЭлементов = Новый Массив();
	МассивЭлементов.Добавить("ГруппаСкопироватьУчетнаяПолитикаБУ");
	МассивЭлементов.Добавить("СкопироватьУчетнаяПолитикаБУ");
	МассивЭлементов.Добавить("ГруппаУчетнаяПолитикаБУОтступ");
	МассивЭлементов.Добавить("ОтступУчетнаяПолитикаБУ");
	МассивЭлементов.Добавить("ОписаниеУчетнаяПолитикаБУ");
	
	СоответствиеЭлементов.Вставить("УчетнаяПолитикаБухУчета", МассивЭлементов);

	МассивЭлементов = Новый Массив();
	МассивЭлементов.Добавить("ГруппаСкопироватьНастройкиУчетаНалогаНаПрибыль");
	МассивЭлементов.Добавить("СкопироватьНастройкиУчетаНалогаНаПрибыль");
	МассивЭлементов.Добавить("ГруппаНастройкиУчетаНалогаНаПрибыльОтступ");
	МассивЭлементов.Добавить("ОтступНастройкиУчетаНалогаНаПрибыль");
	МассивЭлементов.Добавить("ОписаниеНастройкиУчетаНалогаНаПрибыль");
	
	СоответствиеЭлементов.Вставить("НастройкиУчетаНалогаНаПрибыль", МассивЭлементов);

	МассивЭлементов = Новый Массив();
	МассивЭлементов.Добавить("ГруппаСкопироватьНастройкиУчетаЕН");
	МассивЭлементов.Добавить("СкопироватьНастройкиУчетаЕН");
	МассивЭлементов.Добавить("ГруппаНастройкиУчетаЕНОтступ");
	МассивЭлементов.Добавить("ОтступНастройкиУчетаЕН");
	МассивЭлементов.Добавить("ОписаниеНастройкиУчетаЕН");
	
	СоответствиеЭлементов.Вставить("НастройкиУчетаЕН", МассивЭлементов);
    
	
	Возврат СоответствиеЭлементов
	
КонецФункции

